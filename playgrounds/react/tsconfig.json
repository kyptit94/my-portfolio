{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "noUnusedParameters": true,
        "declaration": true,
        "moduleResolution": "node", // Change the value of moduleResolution to "node"
        "resolveJsonModule": true,
        "isolatedModules": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "lib"]
}